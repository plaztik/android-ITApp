<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pzk.itapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Printers"
            android:label="@string/title_activity_printers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ComputerSpecs"
            android:label="@string/title_activity_computer_specs"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pzk.itapp.MainActivity" />
        </activity>
        <activity android:name=".LiveChat" />
        <activity android:name=".LaptopCheckout" />
        <activity
            android:name=".LabInformation"
            android:label="@string/title_activity_lab_information"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>